{
  "name": "kcquest",
  "version": "0.1.0",
  "description": "the KanColle quest Visualization tool",
  "scripts": {
    "start": "npm run dev",
    "dev": "webpack-dev-server --config scripts/webpack.dev.conf.js --mode=development --colors --watch --hot",
    "build": "webpack --config scripts/webpack.prod.conf.js --mode=production --colors",
    "update": "node ./update.js",
    "lint": "eslint --ext .ts,.tsx,.json src",
    "lint:fix": "eslint --ext .ts,.tsx,.json --fix src",
    "typecheck": "tsc --noEmit --project .",
    "clean": "rm -rf dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "KanColle",
    "Quest",
    "Tool"
  ],
  "author": "lawvs",
  "license": "MIT",
  "dependencies": {
    "d3": "^5.12.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/register": "^7.6.0",
    "@babel/runtime": "^7.6.0",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/styled-components": "^4.1.19",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "awesome-typescript-loader": "^5.2.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "eslint": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "fs-extra": "^8.1.0",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "node-fetch": "^2.6.0",
    "prettier": "^1.18.2",
    "typescript": "^3.6.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.2.2"
  }
}
